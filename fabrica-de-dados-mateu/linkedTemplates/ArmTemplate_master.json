{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nome do Data Factory",
			"defaultValue": "fabrica-de-dados-mateu"
		},
		"GoogleSheets1_apiToken": {
			"type": "secureString",
			"metadata": "Cadeia de caracteres segura para 'apiToken' de 'GoogleSheets1'"
		},
		"SqlServerLocalMateu_password": {
			"type": "secureString",
			"metadata": "Cadeia de caracteres segura para 'password' de 'SqlServerLocalMateu'"
		},
		"meudatalake_connectionString": {
			"type": "secureString",
			"metadata": "Cadeia de caracteres segura para 'connectionString' de 'meudatalake'"
		},
		"SqlServerLocalMateu_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "NMB-DADOS-474"
		},
		"SqlServerLocalMateu_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "cervejaria"
		},
		"SqlServerLocalMateu_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "NMB-DADOS-474\\MB Labs"
		},
		"planilhaviahttp_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://docs.google.com/spreadsheets/d/1wWF958qOUIP-kZa7_5ScvxEucDw78wc-OZh-iYX5mw4/export?format=xlsx&gid=0"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI para a conta de armazenamento contendo os modelos do ARM vinculados"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token SAS para a conta de armazenamento contendo modelos do ARM vinculados"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "fabrica-de-dados-mateu_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"GoogleSheets1_apiToken": {
						"value": "[parameters('GoogleSheets1_apiToken')]"
					},
					"meudatalake_connectionString": {
						"value": "[parameters('meudatalake_connectionString')]"
					},
					"SqlServerLocalMateu_password": {
						"value": "[parameters('SqlServerLocalMateu_password')]"
					},
					"planilhaviahttp_properties_typeProperties_url": {
						"value": "[parameters('planilhaviahttp_properties_typeProperties_url')]"
					},
					"SqlServerLocalMateu_properties_typeProperties_server": {
						"value": "[parameters('SqlServerLocalMateu_properties_typeProperties_server')]"
					},
					"SqlServerLocalMateu_properties_typeProperties_database": {
						"value": "[parameters('SqlServerLocalMateu_properties_typeProperties_database')]"
					},
					"SqlServerLocalMateu_properties_typeProperties_userName": {
						"value": "[parameters('SqlServerLocalMateu_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}